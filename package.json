{
  "name": "kaito-leaderboard",
  "version": "1.0.0",
  "description": "Kaito KOL Leaderboard Demo",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "NODE_ENV=development nodemon server.js",
    "production": "NODE_ENV=production node server.js",
    "build": "npm run db:init",
    "query": "node scripts/queryKolData.js",
    "search": "node scripts/queryKolData.js search",
    "stats": "node scripts/queryKolData.js stats",
    "projects": "node scripts/queryKolData.js projects",
    "update-data": "node scripts/update-kol-data.js",
    "test-update": "node scripts/update-kol-data.js --dry-run",
    "db:init": "node scripts/init-database.js",
    "db:import": "node scripts/init-database.js",
    "db:migrate": "npx knex migrate:latest --knexfile database/knexfile.js",
    "db:rollback": "npx knex migrate:rollback --knexfile database/knexfile.js",
    "db:status": "curl -s http://localhost:3000/api/db/stats || echo 'Database not available'"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "knex": "^3.1.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "kaito",
    "crypto",
    "kol",
    "leaderboard"
  ],
  "author": "Your Name",
  "license": "MIT"
}
