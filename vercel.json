{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "demo.html",
      "use": "@vercel/static"
    },
    {
      "src": "kol.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/kol.json",
      "dest": "/kol.json"
    },
    {
      "src": "/(.*)",
      "dest": "/demo.html"
    }
  ]
}
