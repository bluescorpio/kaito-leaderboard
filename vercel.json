{
  "version": 2,
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/kol.json",
      "dest": "/kol.json"
    },
    {
      "src": "/ict.txt",
      "dest": "/ict.txt"
    },
    {
      "src": "/production.html",
      "dest": "/production.html"
    },
    {
      "src": "/demo.html",
      "dest": "/demo.html"
    },
    {
      "src": "/",
      "dest": "/production.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
